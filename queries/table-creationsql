CREATE TABLE sample(
sample_id NUMBER(2) GENERATED AS IDENTITY,
sample_name VARCHAR(20) NOT NULL,
CONSTRAINT PK_SAMPLE_SAMPLEID PRIMARY KEY(sample_id)
)

CREATE TABLE sampleagain(
sample_id NUMBER(2) GENERATED AS IDENTITY,
sample_name VARCHAR(20) NOT NULL
);
ALTER TABLE sampleagain
ADD CONSTRAINT PK_SAMPLEAGAIN_SAMPLEID PRIMARY KEY(sample_id);